    "Language": {
      "attributes": {
        "name": {
          "type": "string",
          "constraint": "UNIQUE",
          "description": "Språk (t.ex. 'Swedish', 'English')"
        }
      }
    },
    "Education": {
      "attributes": {
        "name": {
          "type": "string",
          "constraint": "UNIQUE",
          "description": "Utbildningsnamn (t.ex. 'MSc Teknisk Fysik')"
        },
        "institution": {
          "type": "string",
          "description": "Utbildningsinstitution (t.ex. 'Uppsala Universitet')"
        }
      }
    },
    "Assignment": {
      "attributes": {
        "role": {
          "type": "string",
          "description": "Roll under uppdraget"
        },
        "client": {
          "type": "string",
